{
  "spec_dir": "spec",
  "spec_files": [
    "**/*[sS]pec.?(m)js"
  ],
  "helpers": [
    "helpers/console-reporter.js"
  ],
  "env": {
    "stopSpecOnExpectationFailure": false,
    "random": false
  },
  "preprocessors": {
    "**/*.js": [
      "babel"
    ]
  },
  "reporters": [
    {
      "name": "jasmine-spec-reporter",
      "options": {
        "displayPending": true
      }
    }
  ]
}